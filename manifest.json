{
  "manifest_version": 2,
  "name": "Web Themes Framework",
  "version": "0.3.0",
  "author": "Samantha Glocker",
  "permissions": [],
  "content_scripts": [{
      "matches": [
        "*://www.messenger.com/*",
        "*://www.facebook.com/messages/*",
        "*://discord.com/*",
        "*://web.whatsapp.com/*",
        "*://www.youtube.com/*",
        "*://app.slack.com/*"
      ],
      "js": ["js/injector.js"]
    }],
  "web_accessible_resources": [
    "css/config.css",
    "css/messenger.css",
    "css/discord.css",
    "css/whatsapp.css",
    "css/youtube.css",
    "css/slack.css"
  ]
}
