{
  "manifest_version": 2,
  "name": "Web Themes Framework",
  "version": "0.2.1",
  "author": "Samantha Glocker",
  "permissions": [ "activeTab" ],
  "content_scripts": [{
      "matches": [
        "*://www.messenger.com/*",
        "*://www.facebook.com/messages/*",
        "*://discordapp.com/*",
        "*://web.whatsapp.com/*"
      ],
      "js": ["js/injector.js"]
    }],
  "web_accessible_resources": [
    "css/config.css",
    "css/messenger.css",
    "css/discord.css",
    "css/whatsapp.css"
  ]
}
